<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>FilterChaining</display-name>
  <welcome-file-list>
    <welcome-file>FilterChainingExample</welcome-file>
  </welcome-file-list>
  <filter>
  <filter-name>MyFilter</filter-name>
  <filter-class>com.bridgelabz.programs.MyFilter</filter-class>
  </filter>
  <filter-mapping>
  <filter-name>MyFilter</filter-name>
  <url-pattern>/*</url-pattern>
  </filter-mapping>
  <filter>
  <filter-name>MySecondFilter</filter-name>
  <filter-class>com.bridgelabz.programs.MySecondFilter</filter-class>
  </filter>
  <filter-mapping>
  <filter-name>MySecondFilter</filter-name>
  <url-pattern>/*</url-pattern>
  </filter-mapping>
  <servlet>
  <servlet-name>FilterChaining</servlet-name>
  <servlet-class>com.bridgelabz.programs.FilterChainingExample</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>FilterChaining</servlet-name>
  <url-pattern>/FilterChainingExample</url-pattern>
  </servlet-mapping>
</web-app>